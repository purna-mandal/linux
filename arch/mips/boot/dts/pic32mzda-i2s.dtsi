/ {

	i2s1: i2s@bf821000 {
		compatible = "microchip,pic32-i2s";
		reg = <0xbf821000 512>;
		interrupts = <SPI1_RECEIVE_DONE DEFAULT_INT_PRI IRQ_TYPE_LEVEL_HIGH>,
			<SPI1_TRANSFER_DONE DEFAULT_INT_PRI IRQ_TYPE_LEVEL_HIGH>;
		dmas = <&dma DMA_CHANNEL_0>,
			<&dma DMA_CHANNEL_1>;
		dma-names = "rx", "tx";
		clocks = <&PBCLK2>, <&REFCLKO1>;
		clock-names = "mck0","mck1";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2s1>;
	};

	i2s2: i2s@bf821200 {
		compatible = "microchip,pic32-i2s";
		reg = <0xbf821200 512>;
		interrupts = <SPI2_RECEIVE_DONE DEFAULT_INT_PRI IRQ_TYPE_LEVEL_HIGH>,
			<SPI2_TRANSFER_DONE DEFAULT_INT_PRI IRQ_TYPE_LEVEL_HIGH>;
		dmas = <&dma DMA_CHANNEL_2>,
			<&dma DMA_CHANNEL_3>;
		dma-names = "rx", "tx";
		clocks = <&PBCLK2>, <&REFCLKO1>;
		clock-names = "mck0","mck1";
		status = "disabled";
	};

	i2s3: i2s@bf821400 {
